{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "doctrine/common": "^3.3",
        "drupal/address": "^1.11",
        "drupal/address_suggestion": "^1.0",
        "drupal/belgrade": "^2.0@beta",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/blazy": "^2.16",
        "drupal/block_class": "^2.0",
        "drupal/block_classes": "^1.0",
        "drupal/blockgroup": "^2.0",
        "drupal/bootstrap5_admin": "^1.1",
        "drupal/bootstrap_flag_icons": "^1.0",
        "drupal/bootstrap_layout_builder": "^2.0",
        "drupal/captcha": "^1.5",
        "drupal/charts": "^5.0",
        "drupal/ckeditor_bootstrap_grid": "^1.4",
        "drupal/ckeditor_media_embed": "^1.11",
        "drupal/commerce": "^2.33",
        "drupal/commerce_cart_api": "^1.5",
        "drupal/commerce_cart_flyout": "^1.8",
        "drupal/contact_emails": "^1.20",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.4",
        "drupal/double_field": "^4.0",
        "drupal/entity": "^1.4",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/facets": "^2.0",
        "drupal/features": "^3.12",
        "drupal/feeds": "^3.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/fontawesome": "^2.25",
        "drupal/fontawesome_menu_icons": "^2.0",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.1",
        "drupal/image_slider": "^1.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/leaflet": "^2.1",
        "drupal/metatag": "^1.22",
        "drupal/n1ed": "^2.46",
        "drupal/office_hours": "^1.6",
        "drupal/page_popup": "^1.5",
        "drupal/paragraphs": "^1.14",
        "drupal/pathauto": "^1.10",
        "drupal/pwa_firebase": "2.x-dev@dev",
        "drupal/redis": "^1.6",
        "drupal/restui": "^1.21",
        "drupal/search_api": "^1.23",
        "drupal/search_api_sorts": "^1.0",
        "drupal/shutdown": "^2.0@beta",
        "drupal/simplenews": "3.x-dev",
        "drupal/slick": "^2.9",
        "drupal/slick_views": "^2.7",
        "drupal/sms_message": "^1.0",
        "drupal/smtp": "^1.0",
        "drupal/svg_image": "^1.16",
        "drupal/twig_tweak": "^3.1",
        "drupal/video_embed_facebook": "^1.5",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_data_export": "^1.3",
        "drupal/views_data_export_phpspreadsheet": "^2.0",
        "drupal/views_ical": "1.x-dev",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/views_kanban": "^1.0",
        "drupal/xmlsitemap": "^1.4",
        "drupal/youtube": "^1.2",
        "drush/drush": "^11.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/commerce_cart_flyout": {
                "patch 1": "https://www.drupal.org/files/issues/2023-06-05/arg-mismatch-3364476-2.patch"
            }
        }
    }
}
